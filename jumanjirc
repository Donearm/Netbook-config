#script ~/.config/jumanji/scripts/input_hinting_mode.js
script ~/.config/jumanji/scripts/dwb2_hints.js

set download_dir ~
set download_command urxvtc -e sh -c "wget --load-cookies  ~/.config/jumanji/cookies '%s' -O %s";
set homepage http://www.google.it
searchengine ggl http://www.google.com/search?q=%s
searchengine arch https://bbs.archlinux.org/search.php?q=%s

set default_bgcolor	#252525
set default_fgcolor	#85bdbc
set statusbar_fgcolor #85bdbc
set statusbar_ssl_bgcolor #4a77b2
set completion_g_bgcolor #252525
set inputbar_fgcolor #85bdbc
set inputbar_bgcolor #252525
set tabbar_fgcolor #85bdbc
set tabbar_focus_fgcolor #ffffff
set completion_hl_bgcolor #85bdbc

map <C-q> quitall
